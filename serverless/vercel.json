{
  "routes": [
    {
      "src": "/api/population",
      "dest": "/api/population.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/areas",
      "dest": "/api/areas.js",
      "methods": ["GET"]
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}